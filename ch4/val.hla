program valdemo;
#include ("stdlib.hhf")

// VARIABLES
val
	notConst	:= 0;

// BEGIN
begin valdemo;
	mov (notConst, eax);
	stdout.put ("EAX = ", (type uns32 eax), nl);

	?notConst	:= 10;
	mov (notConst, eax);
	stdout.put ("EAX = ", (type uns32 eax), nl);

	?notConst	:= 20;
	mov (notConst, eax);
	stdout.put ("EAX = ", (type uns32 eax), nl);

	?notConst	:= 30;
	mov (notConst, eax);
	stdout.put ("EAX = ", (type uns32 eax), nl);
// END
end valdemo;

